<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5">

    <include
        android:id="@+id/show_set_title"
        layout="@layout/persoal_title" />
    <RelativeLayout
        android:layout_below="@id/show_set_title"
        android:id="@+id/relativeLayout6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:ignore="UselessParent">
        <ImageView
            android:id="@+id/h_back"
            android:layout_width="match_parent"
            android:layout_height="@dimen/qb_px_185"
            tools:ignore="ContentDescription" />

        <com.nmbb.vlc.Util.CircleImageView
            android:id="@+id/headerimg"
            android:layout_width="@dimen/qb_px_90"
            android:layout_height="@dimen/qb_px_90"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/qb_px_40"
            android:background="@drawable/ic_header"
            tools:ignore="ContentDescription"
            app:border_color="@color/colorPrimary"
            app:border_width="@dimen/qb_px_3"/>
 -->
    </RelativeLayout>
    <TextView
        android:id="@+id/text_xian"
        android:layout_below="@id/relativeLayout6"
        android:layout_width="match_parent"
        android:layout_height="@dimen/qb_px_20"
        android:background="#e6e2e2"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/qb_px_20"
        android:gravity="center"
        android:id="@+id/relativeLayout"
        android:layout_below="@id/text_xian"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_marginLeft="@dimen/qb_px_10"
            android:layout_marginRight="@dimen/qb_px_10"
            android:layout_height="wrap_content"
            android:gravity="center_vertical">

            <TextView
                android:layout_marginLeft="@dimen/qb_px_9"
                android:text="工号："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="@dimen/qb_px_20"
                tools:ignore="HardcodedText,RtlHardcoded,SpUsage" />
            <TextView
                android:id="@+id/job_number"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/qb_px_30"
                android:gravity="center"
                android:layout_marginLeft="@dimen/qb_px_10"
                android:layout_weight="1"
                tools:ignore="InefficientWeight,RtlHardcoded,TextFields" />
        </LinearLayout>
        <TextView
            android:layout_marginTop="@dimen/qb_px_10"
            android:layout_width="match_parent"
            android:layout_height="@dimen/qb_px_1"
            android:background="#6b7479"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_marginLeft="@dimen/qb_px_10"
            android:layout_marginRight="@dimen/qb_px_10"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/qb_px_20"
            android:gravity="center_vertical">

            <TextView
                android:layout_marginLeft="@dimen/qb_px_9"
                android:text="姓名"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="@dimen/qb_px_20"
                tools:ignore="HardcodedText,RtlHardcoded,SpUsage" />
            <TextView
                android:id="@+id/personal_name"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/qb_px_30"
                android:gravity="center"
                android:layout_marginLeft="@dimen/qb_px_10"
                android:layout_weight="1"
                tools:ignore="InefficientWeight,RtlHardcoded,TextFields" />
        </LinearLayout>
        <TextView
            android:layout_marginTop="@dimen/qb_px_10"
            android:layout_width="match_parent"
            android:layout_height="@dimen/qb_px_1"
            android:background="#6b7479"/>
        <LinearLayout
            android:id="@+id/linearlayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/qb_px_10"
            android:layout_marginRight="@dimen/qb_px_10"
            android:layout_marginTop="@dimen/qb_px_20"
            android:gravity="center_vertical">
            <TextView
                android:layout_marginLeft="@dimen/qb_px_9"
                android:text="身份证号"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="@dimen/qb_px_20"
                tools:ignore="HardcodedText,RtlHardcoded,SpUsage" />
            <TextView
                android:layout_marginLeft="@dimen/qb_px_5"
                android:id="@+id/personal_idnum"
                android:layout_width="0dp"
                android:layout_height="@dimen/qb_px_30"
                android:gravity="center"
                android:layout_weight="1"
                tools:ignore="RtlHardcoded" />
        </LinearLayout>
        <TextView
            android:layout_marginTop="@dimen/qb_px_10"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#6b7479"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/qb_px_20"
            android:layout_marginLeft="@dimen/qb_px_10"
            android:layout_marginRight="@dimen/qb_px_10"
            android:gravity="center_vertical">

            <TextView
                android:layout_marginLeft="@dimen/qb_px_9"
                android:text="电话"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/qb_px_20"
                android:gravity="center"
                tools:ignore="HardcodedText,RtlHardcoded,SpUsage" />
            <TextView
                android:layout_marginLeft="@dimen/qb_px_5"
                android:id="@+id/personal_phone"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/qb_px_30"
                android:gravity="center"
                android:layout_weight="1"
                tools:ignore="InefficientWeight,RtlHardcoded,TextFields" />
        </LinearLayout>
        <TextView
            android:layout_marginTop="@dimen/qb_px_10"
            android:layout_width="match_parent"
            android:layout_height="@dimen/qb_px_1"
            android:background="#6b7479"/>
    </LinearLayout>
</RelativeLayout>
